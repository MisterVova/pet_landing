    "template": "<header class=\"header\">\r\n    <div class=\"container\">\r\n        <div class=\"header__inner\">\r\n            <a href=\"#\" class=\"header__nav-logo-link\">\r\n                <img src=\"media/{{kit_of_values.header.imgs.garpix_logo_svg}}\" alt=\"{{kit_of_values.header.imgs.garpix_logo_svg_alt}}\" class=\"header__nav-logo-img\"/>\r\n            </a>\r\n            <nav class=\"header__nav\">\r\n                <ul class=\"header__nav-list\">\r\n                    <li class=\"header__nav-item text3\">\r\n                        <a href=\"#\" class=\"header__nav-item-link\">Кейсы</a>\r\n                    </li>\r\n                    <li class=\"header__nav-item text3\">\r\n                        <a href=\"#\" class=\"header__nav-item-link\">Услуги</a>\r\n                    </li>\r\n                    <li class=\"header__nav-item text3\"><a href=\"#\" class=\"header__nav-item-link\">Блог</a></li>\r\n                </ul>\r\n                <a href=\"{{kit_of_values.header.tel_href}}\" class=\"header__nav-item-link link-tel text3\">{{kit_of_values.header.tel_text}}</a>\r\n                <div class=\"header__burger burger\">\r\n                    <p class=\"burger__text\">Меню</p>\r\n                </div>\r\n            </nav>\r\n            <div class=\"burger__menu\">\r\n                <div class=\"container\">\r\n                    <div class=\"burger__menu__inner\">\r\n                        <div class=\"burger__menu__header\">\r\n                            <div class=\"burger__menu__header-btn burger\">\r\n                                <p class=\"burger__menu__header-btn-text\">Закрыть</p>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"burger__menu__content burger__content\">\r\n                            <div class=\"burger__content__left\">\r\n                                <img\r\n                                        src=\"media/{{kit_of_values.header.imgs.garpix_light_svg}}\"\r\n                                        alt=\"{{kit_of_values.header.imgs.garpix_logo_svg_alt}}\"\r\n                                        class=\"burger__content__left-garpixLogo\"\r\n                                />\r\n                                <button class=\"burger__content__left-btn\">{{kit_of_values.buttons.start_project.text}}</button>\r\n\r\n                                <ul class=\"left__contacts-list\">\r\n                                    <li class=\"left__contacts-item\">\r\n                                        <a href=\"{{kit_of_values.header.tel_href}}\" class=\"left__contacts-link-tel link--tel\"\r\n                                        >{{kit_of_values.header.tel_text}}</a\r\n                                        >\r\n                                    </li>\r\n                                    <li class=\"left__contacts-item\">\r\n                                        <a class=\"left__contacts-link-email link--email\" href=\"mailto:{{kit_of_values.header.contacts_email}}\"\r\n                                        >{{kit_of_values.header.contacts_email}}</a\r\n                                        >\r\n                                    </li>\r\n                                </ul>\r\n\r\n                                <ul class=\"left__contacts-list-social\">\r\n                                    <li class=\"left__contacts-item-social\">\r\n                                        <a href=\"#\" class=\"left__contacts-link link--contact\">Vkontakte</a>\r\n                                    </li>\r\n                                    <li class=\"left__contacts-item-social\">\r\n                                        <a href=\"#\" class=\"left__contacts-link link--contact\">Behance</a>\r\n                                    </li>\r\n                                    <li class=\"left__contacts-item-social\">\r\n                                        <a href=\"#\" class=\"left__contacts-link link--contact\">Youtube</a>\r\n                                    </li>\r\n                                    <li class=\"left__contacts-item-social\">\r\n                                        <a href=\"#\" class=\"left__contacts-link link--contact\">Telegram</a>\r\n                                    </li>\r\n                                    <li class=\"left__contacts-item-social\">\r\n                                        <a href=\"#\" class=\"left__contacts-link link--contact\">Zen</a>\r\n                                    </li>\r\n                                </ul>\r\n                            </div>\r\n                            <div class=\"burger__content__main\">\r\n                                <ul class=\"burger__content__main-list\">\r\n                                    <li class=\"burger__content__main-item\">\r\n                                        <a\r\n                                                href=\"#\"\r\n                                                class=\"burger__content__main-link burger__content__main-link--active\"\r\n                                        >Главная</a\r\n                                        >\r\n                                    </li>\r\n                                    <li class=\"burger__content__main-item\">\r\n                                        <a href=\"#\" class=\"burger__content__main-link\">Кейсы</a>\r\n                                    </li>\r\n                                    <li class=\"burger__content__main-item\">\r\n                                        Услуги\r\n\r\n                                        <ul class=\"burger__content__main__services-list\">\r\n                                            <li class=\"burger__content__main__services-item\">\r\n                                                <a href=\"#\" class=\"burger__content__main__services-link\">Веб-разработка</a>\r\n                                            </li>\r\n                                            <li class=\"burger__content__main__services-item\">\r\n                                                <a href=\"#\" class=\"burger__content__main__services-link\"\r\n                                                >Мобильные приложения</a\r\n                                                >\r\n                                            </li>\r\n                                            <li class=\"burger__content__main__services-item\">\r\n                                                <a href=\"#\" class=\"burger__content__main__services-link\"\r\n                                                >Поддержка и маркетинг</a\r\n                                                >\r\n                                            </li>\r\n                                            <li class=\"burger__content__main__services-item\">\r\n                                                <a href=\"#\" class=\"burger__content__main__services-link\">Аутстафф</a>\r\n                                            </li>\r\n                                        </ul>\r\n                                    </li>\r\n                                    <li class=\"burger__content__main-item\">\r\n                                        <a href=\"#\" class=\"burger__content__main-link\">Карьера</a>\r\n                                    </li>\r\n                                    <li class=\"burger__content__main-item\">\r\n                                        <a href=\"#\" class=\"burger__content__main-link\">Партнерская программа</a>\r\n                                    </li>\r\n                                    <li class=\"burger__content__main-item\">\r\n                                        <a href=\"#\" class=\"burger__content__main-link\">Цифровая академия</a>\r\n                                    </li>\r\n                                    <li class=\"burger__content__main-item\">\r\n                                        <a href=\"#\" class=\"burger__content__main-link\">Блог</a>\r\n                                    </li>\r\n                                </ul>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</header>",
